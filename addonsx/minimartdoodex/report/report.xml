<odoo>
    <record id="report_barang_pdf" model="ir.actions.report">
        <field name="name">Print Laporan Barang</field> 
        <field name="model">doodex.barang</field>    
        <field name="report_type">qweb-pdf</field>  
        <field name="report_name">minimartdoodex.report_barang_template_pdf</field>  
        <field name="report_file">minimartdoodex.report_barang_template_pdf</field>  
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>            
    </record>
            
    <record id="report_jenis_barang_pdf" model="ir.actions.report">
        <field name="name">Print Laporan Barang</field> 
        <field name="model">doodex.jenisbarang</field>    
        <field name="report_type">qweb-pdf</field>  
        <field name="report_name">minimartdoodex.report_jenisbarang_template_pdf</field>  
        <field name="report_file">minimartdoodex.report_jenisbarang_template_pdf</field>  
        <field name="binding_model_id" ref="model_doodex_jenisbarang"/>
        <field name="binding_type">report</field>            
    </record>      

    <record id="report_penjualan_pdf" model="ir.actions.report">
        <field name="name">Print Laporan Penjualan</field> 
        <field name="model">doodex.penjualan</field>    
        <field name="report_type">qweb-pdf</field>  
        <field name="report_name">minimartdoodex.report_penjualan_template_pdf</field>  
        <field name="report_file">minimartdoodex.report_penjualan_template_pdf</field>  
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>            
    </record>        

</odoo>
